<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
	<managed-bean>
		<managed-bean-name>archive</managed-bean-name>
		<managed-bean-class>itemsets.Archive</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>championDetail</managed-bean-name>
		<managed-bean-class>itemsets.ChampionDetail</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>dndView</managed-bean-name>
		<managed-bean-class>itemsets.DNDView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>itemSetDetail</managed-bean-name>
		<managed-bean-class>itemsets.ItemSetDetail</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	

</faces-config>
